Sign Up Here!

<%= form_for(@account, url: accounts_path) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :birthday %>
	<%= f.date_field :birthday %>


	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.label :password_confirmation %>
	<%= f.password_field :password_confirmation %>

	<%= f.submit "Sign Up!" %>
<% end %>